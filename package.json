{
  "name": "klor",
  "version": "0.55.0",
  "description": "syntax highlighting",
  "keywords": [
    "syntax",
    "highlighting"
  ],
  "main": "js/klor.js",
  "files": [
    "js/klor.js",
    "js/kolor.js",
    "js/lang.json"
  ],
  "author": "monsterkodi",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/monsterkodi/klor.git"
  },
  "bugs": {
    "url": "https://github.com/monsterkodi/klor/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha -c --require koffee/js/register ./coffee/test.coffee",
    "watch": "./node_modules/.bin/onchange coffee/test.coffee coffee/klor.coffee -- npm run test",
    "build": "./node_modules/.bin/koffee -o js coffee/*.coffee && npm run lang",
    "lang": "./node_modules/.bin/koffee --lang coffee/klor.coffee"
  },
  "devDependencies": {
    "koffee": ">=0.56.0",
    "kxk": ">=0.304.0",
    "noon": ">=2.10.0",
    "mocha": "^6.1.4",
    "onchange": "^6.0.0"
  }
}